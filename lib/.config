#!/bin/bash
# This is a config file for Linux Live Kit build script.
# You shouldn't need to change anything expect LIVEKITNAME
# mod by pornomo hadi purnomo.shinigami@gmail.com

LIVEKITNAME="dracos" # live kit name look like name in $livedisk/$LIVEKITNAME
DISTNAME="kolomonggo-linux"
# Bundle extension, for example 'sb' for .sb extension


BEXT=sfs # extention for squashfs file 






TGT_TMP_INITRD=/tmp/newinit
TGT_LIVE_DISK=/tmp/live/$(uname -m)
TGT_LIVE_DISK_FILE=/LFS.iso
TGT_SQUASHFS=/tmp/squash
TGT_SQUASHFS_FILE=/tmp/core.$BEXT
SRC_ISO=baseiso/$(uname -m)


INSTALLER_ENABLED="yes"
COMPILE_NEWKERNEL="no"
VERSION_KERNEL=""


